class Solution {
    public int findMinArrowShots(int[][] points) {
        
       
        Arrays.sort(points, (m,n) -> m[0]-n[0]);
        int count=1;
        int end=points[0][1];
        for(int i =1;i<points.length;i++){
            if(points[i][0]>end){
                count++;
                end=points[i][1];
            }
            else{
                end=Math.min(end,points[i][1]);
            }
        }
        return count;
    }
}
