class Solution {
    public int jump(int[] nums) {
        int count=0;
        int max=0;int currentEnd=0;
        if(nums.length==1){
            return 0;
        }
        for(int i=0;i<nums.length-1;i++){
            
            max=Math.max(max,i+nums[i]);

            if(i == currentEnd){
                count += 1;
                currentEnd = max;
            }
            
        }
        return count;
    }
}
