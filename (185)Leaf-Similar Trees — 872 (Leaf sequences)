/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    private void leafcollector(TreeNode node,List<Integer> leaves){
        if(node==null) return;
        if(node.left==null && node.right==null) leaves.add(node.val);
        leafcollector(node.left,leaves);
        leafcollector(node.right,leaves);
    }
    public boolean leafSimilar(TreeNode root1, TreeNode root2) {
        List<Integer> leaves1=new ArrayList<>();
        List<Integer> leaves2=new ArrayList<>();
        leafcollector(root1,leaves1);
        leafcollector(root2,leaves2);
        return leaves1.equals(leaves2);
    }
}


