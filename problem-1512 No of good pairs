class Solution {
    public int numIdenticalPairs(int[] nums) {
        HashMap<Integer,Integer> momap= new HashMap<>();
        int count =0;
        for(int i=0;i<nums.length;i++){
            if(momap.containsKey(nums[i])){
                count+=momap.get(nums[i]);
                momap.put(nums[i],momap.get(nums[i])+1);
            }
            else{
                momap.put(nums[i],1);
            }
        }return count;
    }
}
